{
  "name": "fuser",
  "version": "0.2.1",
  "private": true,
  "description": "FuseR - Fuse Starter Kit with RethinkDB API",
  "scripts": {
    "install": "cd ./server && npm install",
    "createDemoAdmin": "cd ./server && npm run setup",
    "testDemoAdminToken": "cd ./server && npm test",
    "start:db": "cd ./server && rethinkdb --cache-size 512 --server-name rethinkdb",
    "start:server": "cd ./server && npm start",
    "dev:server": "cd ./server && npm run dev",
    "dev:app": "cd ./app && fuse preview",
    "dev:android": "cd ./app && fuse preview -t=Android",
    "dev:ios": "cd ./app && fuse preview -t=iOS"
  },
  "author": "Bartek Kus <bartekus@gmail.com> (https://bartekus.com)",
  "license": "MIT"
}
